@using MoxiWorks.Platform
@using Task = System.Threading.Tasks.Task

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Agent Index</h2>
<table class="table table-striped table-bordered">
    <thead>
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
    </tr>
    </thead>
    @foreach (Agent agent in ViewBag.Agents)
    {

        <tr>
            <td>
                @agent.FirstName
            </td>
            <td>
                @agent.LastName
            </td>
        </tr>

    }


</table>

