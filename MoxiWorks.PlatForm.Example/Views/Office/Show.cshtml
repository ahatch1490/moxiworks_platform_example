@using MoxiWorks.Platform
@using Task = System.Threading.Tasks.Task

@{
    ViewData["Title"] = "Show";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>@ViewBag.Office.Item.Name</h1>

<h2>Agents</h2>
<table class="table">
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
    </tr>
    @foreach (Agent a in ViewBag.Agents.Item.Agents)
    {
      
        <tr>
            <td>@a.FirstName</td>
            <td>@a.LastName</td>
        </tr>
    }
  
</table>